				Language system en-GB {
                }
			}
			sections {
				Frontend section{ 
				   *Pages {
				    *Page: DetailsPage1 from: Component1.backend
				    *Page: IndexPage1
				   }
				}
				Backend section  {
					*Pages {
					*Page: IndexPage1
				   	*Page:  DetailsPage1 
				   	*Page:  IndexPage2
				   	*Page:  DetailsPage2
					}
				}
			}
		}
