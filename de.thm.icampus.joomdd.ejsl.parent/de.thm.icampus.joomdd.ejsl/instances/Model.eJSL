/*
 * This is an example model with the needed model elements entities, pages, and extensions
 * '..' are just placeholders for names and should be replaced in concrete models!
 */
eJSLModel "name of the model" {
	 datatypes {
		Datatype "varchar(255) NOT NULL",
		Datatype "text NOT NULL",
		Datatype "datetime NOT NULL DEFAULT '0000-00-00 00:00:00'",
		Datatype "date",
		Datatype "textarea",
		Datatype "text",
		Datatype "editor"		
	}
	entities { 
		Entity Person {
			attributes {
				Attribute Vorname {
					dbtype = "varchar(255) NOT NULL"
					htmltype = "text"
					Primary attribute
				}
				Attribute Name {
					dbtype = "varchar(255) NOT NULL"
					htmltype = "editor"
					Primary attribute
				}
			}
		}
		Entity entity2 {
			attributes { 
				Attribute attribute1 {
					dbtype = "varchar(255) NOT NULL"
					htmltype = "text"
					Primary attribute
				}
				Attribute attribute2 {
					dbtype = "varchar(255) NOT NULL"
					htmltype = "text"
				}
			}
			references {
				Reference {
					Attribute = attribute2
					*Entity = Person
					Reference = Person.Name
					lower = 1
					upper = 1
				}
			}
		}
	}

	pages {
		IndexPage IndexPage1 {
			*Entities Person
			table columns = Person.Vorname, 
				Person.Name
		filters = Person.Name ,Person.Vorname
		links {
			InternalLink Test {
				target = * DetailsPage1
				linked attribute = * Person.Name
			}
		}
		}
		DetailsPage DetailsPage1 {
			*Entities Person
			links {
				InternalLink forall {
					target = * IndexPage1
					linked attribute = * Person.Name
				}
			}
		}
		IndexPage IndexPage2 {
			*Entities Person
			table columns =  Person.Name, Person.Vorname
		filters = Person.Name , Person.Vorname
				links {
					InternalLink Link {
						target = * DetailsPage1
						linked attribute = * Person.Name
					}
				}
				
		}
		DetailsPage DetailsPage2 {
			*Entities entity2
			links {
				InternalLink forall {
					target = * IndexPage2
					linked attribute = * entity2.attribute1
				}
			} 
		}
	}
	extensions {
		Component Component1 {
			Manifestation {
				authors {
					Author "forename surname" {
						authoremail = "name@provider.domains"
						authorurl = "www.anyurl.com"
					}
				} 
				creationdate=20.04.1222			
