//-begin terminals

terminal MENULEVEL:
	('0'..'1')('0') | ('1'..'9')
;

terminal DATE:
	((('0'..'2') ('0'..'9') '.' ('02')) |
	(('0'..'2') ('0'..'9') '.' ((('0') (('1') | ('3'..'9'))) | ('1') ('0'..'2')) | (('1') ('0'..'2'))) |
	(('30') '.' ((('0') (('1') | ('3'..'9'))) | ('1') ('0'..'2'))) |
	(('31') '.' ((('0') (('1') | ('3') | ('5') | ('7') | ('8'))) | (('1') (('0')| ('2'))))))
	'.' ('0'..'2') ('0'..'9') ('0'..'9') ('0'..'9')
;

terminal DATETIME:
	((('0'..'2') ('0'..'9') '.' ('02')) |
	(('0'..'2') ('0'..'9') '.' ((('0') (('1') | ('3'..'9'))) | ('1') ('0'..'2')) | (('1') ('0'..'2'))) |
	(('30') '.' ((('0') (('1') | ('3'..'9'))) | ('1') ('0'..'2'))) |
	(('31') '.' ((('0') (('1') | ('3') | ('5') | ('7') | ('8'))) | (('1') (('0')| ('2'))))))
	'.' ('0'..'2') ('0'..'9') ('0'..'9') ('0'..'9')
	' ' ((('2') ('0'..'3')) | (('0'..'1') ('0'..'9'))) ':' ('0'..'5') ('0'..'9') ':' ('0'..'5') ('0'..'9')
;

//- end terminals

