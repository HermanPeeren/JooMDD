				Attribute modified: "datetime NOT NULL DEFAULT \'0000-00-00 00:00:00\'"
				Attribute modified_by: "int(11) NOT NULL"
				Attribute metakey: "text NOT NULL"
				Attribute metadesc: "text NOT NULL"
				Attribute metadata: "text NOT NULL"
				Attribute featured: "tinyint(1) NOT NULL DEFAULT"
