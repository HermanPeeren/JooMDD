grammar de.thm.icampus.cjsl.Reverse with org.eclipse.xtext.common.Terminals

generate reverse "http://www.thm.de/icampus/cjsl/Reverse"

Application:
	"Application: " name=ID
	"The source path of the Joomla core: " joomlaPath=STRING
	"The destination path for the Application: " applicationPath=STRING
	cjsl_configuration=cJSL_Configuration
	cjsl_user=cJSL_User
	cjsl_content=cJSL_Content
	cjsl_page=cJSL_Page
	cjsl_menu=cJSL_Menu
;

//- begin configuration

cJSL_Configuration:
	"cJSL_Configuration {"
	(
		website_conf=website_Conf
		&db_conf=db_Conf
		&(ftp_conf=ftp_Conf)?
		&(mailer_conf=mailer_Conf)?
		&(system_conf=system_Conf)
		&(editor+=Editor)*
		&(viewLevel+=ViewLevel)*
		&(template+=Template)*
		&(language+=Language)*
		&(help_server=HelpServer)?
		&(timeZone+=TimeZone)*
	)
	"}"
;

website_Conf:
	"Website Configuration ("
		"Page title:" pagetitle=STRING
		"Admin:" superadmin=User
	(
		("Example data:" exampledata=("yes"|"no"))?
		&("Offline:" offline=("yes"|"no"))?
		&("Offline message:" offline_message=STRING)?
		&("Display offline message:" display_offline_message=domType)?
		&("Offline image:" offline_image=STRING)?
		&("*Editor:" editor= EditorTypeReference)?
		&("Captcha:" captcha=STRING)?
		&("Site access:" site_access=ViewLevelRights)?
		&("List limit:" list_limit=INT)?
		&("Feed limit:" feed_limit=INT)?
		&("Feed email:" feed_email=feedEmailType)?
		&("Description:" description=STRING)?
		&("Keyword:" keyword+=STRING)?
		&("Robot:" robot=robotType)?
		&("Content right:" content_right=STRING)?
		&("Show author meta tag:" show_author_meta_tag=("yes"|"no"))?
		&("Show Joomla version:" show_joomla_version=("yes"|"no"))?
		&("Use SEF-URLs:" use_sef=("yes"|"no"))?
		&("URL rewrite:" url_rewrite=("yes"|"no"))?
		&("Add suffix to URL:" add_suffix_to_url=("yes"|"no"))?
		&("Unicode aliases:" unicode_aliases=("yes"|"no"))?
		&("Include site name in page titles:" include_site_name_in_page_titles=inclSiteNameInPT)?
		&("Cookie domain:" cookie_domain=STRING)?
		&("Cookie path:" cookie_path=STRING)?
	)
	")"
;

mailer_Conf:
	"Mailer Configuration ("
		"Mailer:" mailer=mailerType
		"Mail from:" mail_from=STRING
		"Sender Name:" name_from=STRING
	(
		("Sendmail path:" sendmail_path=STRING)?
		&(smtpconfig=smtp_Conf)?
	)
	")"
;

smtp_Conf:
	"SMTP Configuration ("
		"SMTP authentication:" smtp_auth=("yes"|"no")
		"SMTP security:" smtp_security=smtpSecurityType
		"SMTP port:" smtp_port=INT
		"SMTP username:" smtp_username=STRING
		"SMTP password:" smtp_password=STRING
		"SMTP host:" smtp_host=HostValueType
	")"
;

system_Conf:
	"System Configuration ("
		"Temp path:" temp_path=STRING
		("Gzip compatibility:" gzip_compatibility=("yes"|"no"))?
		("Error reporting type:" error_reporting_type=errorReportingType)?
		("Force SSL:" force_ssl=forceSslType)?
		("*Server time zone:" server_time_zone=[TimeZone])?
		"Log path:" log_path=STRING
		("*Help server:" help_server=[HelpServer])?
		("Debug system:" debug_system=("yes"|"no"))?
		("Debug language:" debug_language=("yes"|"no"))?
		"Caching:" caching=cachingType
		("Cache handler:" cache_handler=cacheHandlerType)?
		"Cache time:" cache_time=INT
		"Session lifetime:" session_lifetime=INT
		("Session handler:" session_handler=sessionHandlerType)?
	")"
;

ftp_Conf:
	"FTP Configuration ("
		"Enable:" enable=("yes"|"no")
	(
		("Host:" host=HostValueType)?
		&("Port:" port=INT)?
		&("FTP user:" ftp_user=ID)?
		&("FTP password:" ftp_pass=ID)?
		&("Root path:" root_path=ID)?
	)
	")"
;

db_Conf:
	"DB Configuration ("
		"Database:" database=ID
		"User:" user=ID
		"Password:" password=STRING
		"Type:" dbtype=databaseType
	(
		("Prefix:" prefix=ID)?
		&("Host:" host=HostValueType)?
	)
	")"
;

Template:
	"Template definition" name=ID "("
		"Name:" title=STRING
		position+=Position*
	")"
;

Position:
	"Position:" name=ID
;

EditorTypeReference:
	EditorReference | DefaultEditorReference
;

EditorReference:
	reference=[Editor]
;

DefaultEditorReference:
	reference = defaultEditor
;

Editor:
	"Editorname: " name=ID
;
ViewLevelRights:
	DefaultViewLevelReference | ViewLevelReference
;
ViewLevelReference:
	ref=[ViewLevel]
;

DefaultViewLevelReference:
	ref = defaultViewLevel
;

LanguageTypeReference:
	LanguageReference | DefaultLanguageReference
;

LanguageTypeReferenceWithAll:
	LanguageTypeReference | all="All"
;

LanguageReference:
	reference=[Language]
;

DefaultLanguageReference:
	reference=defaultLanguage
;

Language:
	"Language: " name=ID
;

HelpServer:
	"Help server" name=ID "("
		"*Language:" language=LanguageTypeReference
		"Title:" title=STRING
		"Link:" link=STRING
	")"
;

TimeZone:
	"Timezone"  name = ID "("
		"Continent:" continent=STRING
		"Country:" country=STRING
	")"
;

//- end configuration

//- begin page

cJSL_Page:
	"cJSL_Page {"
		page+=Page+
	"}"
;

Page:
	ArticlePage | UserPage | ContactPage | WebLinksPage | NewsFeedPage | SmartSearchPage | SearchPage | IFrameWrapperPage
;

ArticlePage:
	ArchivedArticlesPage | SingleArticlePage | ListAllCategoriesPage | CategoryPage | FeaturedArticlesPage | CreateArticlePage
;

ArchivedArticlesPage:
	"Archived articles:" name=ID "("
	(
		("Article order:" articleOrder=articleOrderType)?
		&("Date for ordering:" orderDate=orderDateType)?
		&("Articles to list:" articlesToList=articlesToListType)?
		&("Filter field:" filterField=optionSettingType)?
		&("Intro text limit:" introTextLimit=INT)?
		&(articleLayoutOptions=ArticleSettingsOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

SingleArticlePage:
	"Single article:" name=ID "("
	(
		("*Article:" content=[Content])
		&(articleSettingsOptons=ArticleSettingsOptions)?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

ListAllCategoriesPage:
	"List all categories:" name=ID "("
	(
		("*Root category:" rootCategory=[ArticleCategory])?
		&("Categories options {"
			categoriesOptions=CategoriesOptions
		"}")?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Blog layout options {"
			blogLayoutOptions=BlogLayoutOptions
		"}")?
		&("Ordering pagination options {"
			orderingPaginationOptions=OrderingPaginationOptions
		"}")?
		&("List layout options {"
			listLayoutOptions=ListLayoutOptions
		"}")?
		&(articleSettingsOptons=ArticleSettingsOptions)?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

CategoryPage:
	"Category page:" name=ID "("
	(
		("*Category:" category=[ArticleCategory])
		&("Show subcategories text:" showSubcatText=optionSettingType)?
		&("Page subheading:" pageSubheading=STRING)?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Article layout options {"
			articleLayoutOptions=ArticleLayoutOptions
		"}")?
		&("Ordering pagination options {"
			orderingPaginationOptions=OrderingPaginationOptions
		"}")?
		&(articleSettingsOptons=ArticleSettingsOptions)?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

FeaturedArticlesPage:
	"Featured articles:" name=ID "("
	(
		("*Categories:" categories+=[ArticleCategory])*
		&("Layout options {"
			layoutOptions=BlogLayoutOptions
		"}")?
		&("Ordering pagination options {"
			orderingPaginationOptions=OrderingPaginationOptions
		"}")?
		&(articleSettingsOptons=ArticleSettingsOptions)?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

CreateArticlePage:
	"Create article:" name=ID "("
	(
		(
			("Just default category:" justDefaultCategory=("yes"|"no"))
			("*Default category:" defaultCategory=[ArticleCategory])
		)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

UserPage:
	LoginPage | UserProfilePage | EditUserProfilePage | RegistrationPage | UsernameReminderRequestPage | PasswordResetPage
;

LoginPage:
	"Login page:" name=ID "("
	(
		("Login redirect:" loginRedirect=STRING)?
		&(
			("Show login description:" showLoginDescription=optionSettingType)
			("Login description text:" loginDescriptionText=STRING)
		)?
		&("Login image:" loginImage=STRING)?
		&("Logout redirect:" logoutRedirect=STRING)?
		&(
			("Show logout description:" showLogoutDescription=optionSettingType)
			("Logout description text:" logoutDescriptionText=STRING)
		)?
		&("Logout image:" logoutImage=STRING)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

UserProfilePage:
	"User profile page:" name=ID "("
		metadataOptions=MetadataOptions?
	")"
;

EditUserProfilePage:
	"Edit user profile:" name=ID "("
		metadataOptions=MetadataOptions?
	")"
;

RegistrationPage:
	"Registration page:" name=ID "("
		metadataOptions=MetadataOptions?
	")"
;

UsernameReminderRequestPage:
	"Username reminder request:" name=ID "("
		metadataOptions=MetadataOptions?
	")"
;

PasswordResetPage:
	"Password reset page:" name=ID "("
		metadataOptions=MetadataOptions?
	")"
;

ContactPage:
	ContactListAllCategoriesPage | ListContactInCategoryPage | SingleContactPage | FeaturedContactPage
;

ContactListAllCategoriesPage:
	"List all categories (Contact):" name=ID "("
	(
		("*Root category:" rootCategory=[ContactCategory])?
		&("Categories options {"
			categoriesOptions=CategoriesOptions
		"}")?
		&("Top level category description:" topLevelCatDesc=STRING)?
		&("Show contacts in category:" showContactsInCategory=optionSettingType)?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Contact list Layout options {"
			contactListLayout=ContactListLayoutOptions
		"}")?
		&("Contact display options {"
			contactDisplayOptions=ContactDisplayOptions
		"}")?
		&("Contact Email options {"
			contactEmailOptions=ContactEmailOptions
		"}")?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

ListContactInCategoryPage:
	"List contact in a category:" name=ID "("
	(
		("*Category:" category=[ContactCategory])?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Show contacts in category:" showContactsInCategory=optionSettingType)?
		&("Contact list Layout options {"
			contactListLayout=ContactListLayoutOptions
		"}")?
		&("Contact display options {"
			contactDisplayOptions=ContactDisplayOptions
		"}")?
		&("Contact Email options {"
			contactEmailOptions=ContactEmailOptions
		"}")?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

SingleContactPage:
	"Single contact:" name=ID "("
	(
		("*Contact:" contact=[Contact])?
		&("Contact display options {"
			contactDisplayOptions=ContactDisplayOptions
		"}")?
		&("Contact Email options {"
			contactEmailOptions=ContactEmailOptions
		"}")?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

FeaturedContactPage:
	"Featured contact page:" name=ID "("
	(
		("Contact list Layout options {"
			contactListLayout=ContactListLayoutOptions
		"}")?
		&("Contact display options {"
			contactDisplayOptions=ContactDisplayOptions
		"}")?
		&("Contact Email options {"
			contactEmailOptions=ContactEmailOptions
		"}")?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

WebLinksPage:
	ListAllWeblinkCategoriesPage | ListWeblinkCategoryPage | WeblinkSubmitPage
;

ListAllWeblinkCategoriesPage:
	"List all weblink categories:" name=ID "("
	(
		("*Root category:" rootCategory=[WeblinkCategory])?
		&("Categories options {"
			categoriesOptions=CategoriesOptions
		"}")?
		&("Top level category description:" topLevelCatDesc=STRING)?
		&("Show the number of weblinks:" showNumberOfWeblinks=optionSettingType)?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Show weblink in category:" showWeblinkInCategory=optionSettingType)?
		&("Weblink List layout options {"
			weblinkListLayoutOptions=WeblinkListLayoutOptions
		"}")?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

ListWeblinkCategoryPage:
	"List a weblink category:" name=ID "("
	(
		("*Category:" category=[WeblinkCategory])?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Show weblink in category:" showWeblinkInCategory=optionSettingType)?
		&("Weblink List layout options {"
			weblinkListLayoutOptions=WeblinkListLayoutOptions
		"}")?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

WeblinkSubmitPage:
	"Submit a weblink:" name=ID "("
	(
		(metadataOptions=MetadataOptions)?
	)
	")"
;

NewsFeedPage:
	ListAllNewsfeedCategoriesPage | ListNewsfeedInCategoryPage | SingleNewsfeedPage
;

ListAllNewsfeedCategoriesPage:
	"List all newsfeed categories:" name=ID "("
	(
		("*Root category:" rootCategory=[NewsfeedCategory])?
		&("Categories options {"
			categoriesOptions=CategoriesOptions
		"}")?
		&("Top level category description:" topLevelCatDesc=STRING)?
		&("Show the number of newsfeeds:" showNumberOfNewsfeeds=optionSettingType)?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Show newsfeed in category:" showNewsfeedInCategory=optionSettingType)?
		&("Newsfeed List layout options {"
			newsfeedListLayoutOptions=NewsfeedListLayoutOptions
		"}")?
		&("Newsfeed display options {"
			newsfeedDisplayOptions=NewsfeedDisplayOptions
		"}")?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

ListNewsfeedInCategoryPage:
	"List a newsfeed in a category:" name=ID "("
	(
		("*Category:" category=[NewsfeedCategory])?
		&("Category options {"
			categoryOptions=CategoryOptions
		"}")?
		&("Show contacts in category:" showContactsInCategory=optionSettingType)?
		&("Newsfeed List layout options {"
			newsfeedListLayoutOptions=NewsfeedListLayoutOptions
		"}")?
		&("Newsfeed display options {"
			newsfeedDisplayOptions=NewsfeedDisplayOptions
		"}")?
		&("Feed display order:" feedDisplayOrder=feedDisplayOrderType)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

SingleNewsfeedPage:
	"Single newsfeed:" name=ID "("
	(
		("*Newsfeed:" newsfeed=[Newsfeed])?
		&("Newsfeed display options {"
			newsfeedDisplayOptions=NewsfeedDisplayOptions
		"}")?
		&("Feed display order:" feedDisplayOrder=feedDisplayOrderType)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

SmartSearchPage:
	"Smart search page:" name=ID "("
	(
		("Search query:" searchQuery=STRING)?
		&("Search filter:" searchFilter=STRING)?
		&("Show date filters:" showDateFilters=optionSettingType)?
		&("Show advanced search:" showAdvancedSearch=optionSettingType)?
		&("Expand advanced search:" expandAdvancedSearch=optionSettingType)?
		&(
			("Show result description:" showResultDescription=optionSettingType)
			("Result description:" reslutDescription=STRING)
			("Description length:" descriptionLength=INT)?
		)?
		&("Show result URL:" showResultUrl=optionSettingType)?
		&("Show pagination limit:" showPaginationLimit=optionSettingType)?
		&("Show pagination:" showPagination=optionSettingType)?
		&("Show pagination results:" showPaginationResults=optionSettingType)?
		&("Allow empty search:" allowEmptySearch=("yes"|"no"))?
		&("Sort field:" sortField=sortFieldType)?
		&("Sort direction:" sortDirection=sortDirectionType)?
		&("Show feed:" showFeed=optionSettingType)?
		&("Show feed text:" showFeedText=("yes"|"no"))?
		&(integrationOptions=IntegrationOptions)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

SearchPage:
	"Search page:" name=ID "("
	(
		("Search term:" searchTerm=STRING)?
		&("Use search areas:" useSearchAreas=optionSettingType)?
		&("Show creation date:" showCreationDate=optionSettingType)?
		&("Search for:" searchFor=searchForType)?
		&("Results ordering:" searchResultsOrdering=searchResultsOrderingType)?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

IFrameWrapperPage:
	"IFrame wrapper:" name=ID "("
	(
		("URL:" Url=STRING)
		&("Scroll bars:" scrollBars=scrollBarsType)?
		&("Width:" width=STRING)?
		&("Height:" height=STRING)?
		&("Auto height:" autoHeight=("yes"|"no"))?
		&("Auto add:" autoAdd=("yes"|"no"))?
		&("Frame border:" frameBorder=("yes"|"no"))?
		&(metadataOptions=MetadataOptions)?
	)
	")"
;

//- end page

//- begin user

cJSL_User:
	"cJSL_User {"
		usergroups+=UserGroup*
		user+=User+
		viewlevel+=ViewLevel*
	"}"
;

CrudAccessLevel:
	"Access level ("
	(
		("*User group:" userGroup=[UserGroup])
		&("Create:" create=accessLevelRight)?
		&("Delete:" delete=accessLevelRight)?
		&("Edit:" edit=accessLevelRight)?
		&("Edit State:" edit_state=accessLevelRight)?
		&("Edit Own:" edit_own=accessLevelRight)?
	)
	")"
;

UserGroup:
	"User Group:" name=ID "("
		("*Parent:" parent=[UserGroup])?
		(globalAccess_level=CoreAccessLevelOptions)?
	")"
;

User:
	"User:" name=ID "("
		"Name:" fullname=STRING
		"EMail:" email=STRING
	(
		("*UserGroups:" usergroup+=[UserGroup] ("," usergroup+=[UserGroup])*)?
		&("Password:" password=STRING)?
		&("Blocked:" blocked=("yes"|"no"))
		&("Receive system mail:" receiveSystemMail=("yes"|"no"))
		&("*UserGroups:" usergroup+=[UserGroup] ("," usergroup+=[UserGroup])*)?
		&("*Backend template style:" backendTemplateStyle=[Template])?
		&("*Backend language:" backendLanguage=LanguageTypeReference)?
		&("*Frontend language:" frontendLanguage=LanguageTypeReference)?
		&("*Editor:" editor=EditorTypeReference)?
		&("*Help server:" help_server=[HelpServer])?
		&("*Time zone:" time_zone=[TimeZone])?
		&("Register date:" registerDate=DATETIME)?
		&("Last visit date:" lastVisitDate=DATETIME)?
		&("Reset count:" resetCount=INT)?
		&("Last reset:" lastResetTime=DATETIME)?
		&("Activation:" activation=STRING)?
		&userProfile=UserProfile?
	)
	")"
;

UserProfile:
	{UserProfile}
	"User profile: ("
	(
		"Address 1:" address1=STRING?
		&"Address 2:" address2=STRING?
		&"City:" city=STRING?
		&"Region:" region=STRING?
		&"Postal/ZIP code:" zipCode=INT?
		&"Phone:" phone=STRING?
		&"Website:" website=STRING?
		&"Favorite book:" favBook=STRING?
		&"About me:" aboutMe=STRING?
		&"Date of birth:" dateOfBirth=DATE?
	)
	")"
;

ViewLevel:
	"View level" name=ID "("
	(
		("Title:" title=STRING)?
		&("*UserGroups:" usergroup+=[UserGroup] ("," usergroup+=[UserGroup])*)?

	)
	")"
;

//- end user

//- begin content

cJSL_Content:
	"cJSL_Content {"
		articleCategory+=ArticleCategory+
	(
		("Global Article category options {"
			globalArticleCategoryOptions=CategoryOptions
		"}")?
		&("Global Banners category options {"
			globalBannersCategoryOptions=CategoryOptions
		"}")?
		&("Global Weblink category options {"
			globalWeblinkCategoryOptions=CategoryOptions
		"}")?
		&("Global Newsfeed category options {"
			globalNewsfeedCategoryOptions=CategoryOptions
		"}")?
		&("Global Contact category options {"
			globalContactCategoryOptions=CategoryOptions
		"}")?
		&("Global Category options {"
			globalUserCategoryOptions=CategoryOptions
		"}")?
		&("Global Content options {"
			globalContentOptions=ArticleSettingsOptions
		"}")?
		&("Global Newsfeed options {"
			globalNewsfeedOptions=NewsfeedDisplayOptions
		"}")?
		&("Global Contact options {"
			globalContactOptions=ContactDisplayOptions
		"}")?
	)
	(
		(bannerCategory+=BannerCategory)
		| (weblinkCategory+=WeblinkCategory)
		| (newsfeedCategory+=NewsfeedCategory)
		| (contactCategory+=ContactCategory)
		| (userCategory+=UserCategory)
		| (content+=Content)
		| (newsfeed+=Newsfeed)
		| (weblink+=Weblink)
		| (contact+=Contact)
		| (bannerClient+=BannerClient)
		| (banner+=Banner)
		| (bannerTracks+=BannerTracks)
	)*
	"}"
;

ArticleCategory:
	"Article category:" name=ID "("
	(
		("*Parent:" parent=[ArticleCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout=categoryType)?
	)
	")"
;

BannerCategory:
	"Banners category:" name=ID "("
	(
		("*Parent:" parent=[BannerCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout=categoryType)?
	)
	")"
;

WeblinkCategory:
	"Weblink category:" name=ID "("
	(
		("*Parent:" parent=[WeblinkCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout="Default")?
	)
	")"
;

ContactCategory:
	"Contact category:" name=ID "("
	(
		("*Parent:" parent=[ContactCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout="Default")?
	)
	")"
;

UserCategory:
	"User category:" name=ID "("
	(
		("*Parent:" parent=[UserCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout="Default")?
	)
	")"
;

NewsfeedCategory:
	"Newsfeed category:" name=ID "("
	(
		("*Parent:" parent=[NewsfeedCategory])?
		&("Category options {"
			catOptions=ContentCategoryOptions
		"}")?
		&("Alternative layout:" alternativeLayout="Default")?
	)
	")"
;

Content:
	"Content:" name=ID "("
	(

		("Title:" title=STRING)?
		&("Article text:" articleText=ContentBlock)
		&("Intro text:" introText=ContentBlock)?
		&("*Author:" author=[User])?
		&("Author alias:" authorAlias=STRING)?
		&("*Category:" category=[ArticleCategory])
		&("*View level:" viewlevel=[ViewLevelRights])?
		&("Status:" status=contentStatusType)?
		&("Access level {"
			access_level=CrudAccessLevel
		"}")?
		&("SectionId:" sectionId=STRING)?
		&("Mask:" mask=STRING)?
		&("Creation date:" creationDate=DATETIME)?
		&("Modification date:" modificationDate=DATETIME)?
		&("*Modified by:" modifiedBy=[User])?
		&("Checked out:" checkedOut=("yes"|"no"))?
		&("Checked out time:" checkedOutTime=DATETIME)?
		&("Start publishing:" startPublishing=DATETIME)?
		&("Finish publishing:" finishPublishing=DATETIME)?
		&("Images options {"
			imagesOptions=ContentImageOptions
		"}")?
		&("URL options {"
			urlOptions=ContentUrlSettingOptions
		"}")?
		&("Attribute options {"
			attributeOptions=ContentAttributeOptions
		"}")?
		&("Version:" version=STRING)?
		&("*Parent article:" parentArticle=[Content])?
		&("Ordering:" ordering=INT)?
		&("Hits:" hits=INT)?
		&("Content metadata options {"
			contentMetadataOptions=ContentMetaDataOptions
		"}")?
